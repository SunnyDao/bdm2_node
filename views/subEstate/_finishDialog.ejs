<!-- 完善情况对话框 -->
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4>完善情况</h4>
    </div>
    <div class="modal-body">
        <form class="validate">
            <div class="row">
                <div class="col-xs-3">
                    <label>完善状态<span class="text-danger">*</span>：</label>
                </div>
                <div class="col-xs-9">
                    <label>
                        <input type="radio" name="perfectStatus" value="1" />未完善</label>
                    <label>
                        <input type="radio" name="perfectStatus" value="2" />已完善</label>
                </div>
            </div>
            <div class="row channels">
                <div class="col-xs-3">
                    <label>完善渠道<span class="text-danger">*</span>：</label>
                </div>
                <div class="col-xs-9">                
                    <label>
                        <input type="checkbox"  value="1" name="tags" data-validation-engine="validate[required]" />盘丝洞</label>
                    <label>
                        <input type="checkbox"  value="2" name="tags" data-validation-engine="validate[required]"/>竞网</label>
                    <label>
                        <input type="checkbox"  value="3" name="tags" data-validation-engine="validate[required]"/>直营反馈</label>
                    <label>
                        <input type="checkbox"  value="4" name="tags" data-validation-engine="validate[required]"/>wp反馈</label>
                    <label>
                        <input type="checkbox"  value="5" name="tags" data-validation-engine="validate[required]"/>笋盘</label>
                    <label>
                        <input type="checkbox"  value="6" name="tags" data-validation-engine="validate[required]"/>其他</label>
                        <input name="channel" type="hidden"/>
                        <input name="subEstateId" type="hidden"/>
                </div>
            </div>
            <div class="row reason">
                <div class="col-xs-3">
                    <label>完善理由：</label>
                </div>
                <div class="col-xs-9">                
                    <textarea name="note" class="form-control"></textarea>
                </div>
            </div>
            <br/>
            <br/>
            <div class="text-center">
                <button type="button" class="btn btn-primary btn-confirm">确定</button>
                <button type="button" class="btn btn-default" data-dismiss="modal">取消</button>
            </div>
        </form>
        <h4>历史修改记录：</h4>
        <table id="finishList">
        	
        </table>
    </div>
